{
  "name": "lou.network",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=* npx nodemon -e js,json,pem,env ./bin/www",
    "start": "DEBUG=routes:*,app:*,..:*,node_modules:bizi-irc:*,oauth2:*,express:* node ./bin/www",
    "login": "gcloud auth print-access-token | sudo docker login -u oauth2accesstoken --password-stdin https://us-central1-docker.pkg.dev",
    "build": "sudo docker build . -t lou-network",
    "push": "sudo docker tag lou-network us-central1-docker.pkg.dev/progressive-web-dev-300511/gabe-docker/lou-network:latest && sudo docker push us-central1-docker.pkg.dev/progressive-web-dev-300511/gabe-docker/lou-network",
    "deploy": "npm run build && npm run push && ssh debian@lou-remote \"./updt.sh\""
  },
  "dependencies": {
    "@node-oauth/express-oauth-server": "^4.1.0",
    "@node-oauth/oauth2-server": "^5.2.0",
    "@phc/pbkdf2": "^1.1.14",
    "@types/express": "^4.17.21",
    "@types/nodemailer": "^6.4.15",
    "@types/sequelize": "^4.28.20",
    "@types/vue": "^2.0.0",
    "bizi-irc": "file:bin/bizi-irc-0.0.4.tgz",
    "callsite": "^1.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cssnano-preset-advanced": "^7.0.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "escape-regex-string": "^1.0.6",
    "express": "~4.16.1",
    "express-promise-router": "^4.1.1",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "inline-css": "^4.0.2",
    "jose": "^5.6.3",
    "jstransformer-markdown-it": "^3.0.0",
    "jstransformer-postcss": "^1.3.0",
    "minimatch": "^10.0.1",
    "morgan": "~1.9.1",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.14",
    "postcss-preset-env": "^10.0.0",
    "pug": "2.0.0-beta11",
    "sasl-scram-sha-1": "^1.3.0",
    "saslmechanisms": "^0.1.1",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
